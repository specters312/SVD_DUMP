            <field>
              <name>IGNACK</name>
              <description>When set, the received NACK field is ignored and assumed to be ACK</description>
              <bitOffset>9</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
