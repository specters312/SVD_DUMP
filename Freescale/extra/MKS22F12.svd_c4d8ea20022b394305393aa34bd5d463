                  <value>#1000</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1001</name>
                  <description>Channel 0 or Channel 3 pin input capture</description>
                  <value>#1001</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1010</name>
                  <description>Channel 1 or Channel 3 pin input capture</description>
                  <value>#1010</value>
                </enumeratedValue>
